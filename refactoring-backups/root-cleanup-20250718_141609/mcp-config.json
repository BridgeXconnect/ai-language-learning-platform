{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "magic": {
      "command": "npx",
      "args": ["-y", "@21st-dev/magic"]
    },
    "playwright": {
      "command": "npx",
      "args": ["-y", "@playwright/mcp"]
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem"]
    },
    "archon-agents": {
      "command": "python",
      "args": [
        "/Users/roymkhabela/Downloads/AI Language Learning Platform/agents/orchestrator/mcp_server.py"
      ],
      "env": {
        "PYTHONPATH": "/Users/roymkhabela/Downloads/AI Language Learning Platform/agents/orchestrator",
        "SUPABASE_URL": "${SUPABASE_URL}",
        "SUPABASE_KEY": "${SUPABASE_KEY}",
        "OPENAI_API_KEY": "${OPENAI_API_KEY}"
      }
    }
  }
}